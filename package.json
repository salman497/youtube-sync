{
  "name": "youtube-sync",
  "version": "1.0.0",
  "description": "YouTube video to MP3 converter",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "dev:client": "cd client && npm start",
    "youtube:to:mp3": "ts-node src/utils/convert-youtube-video-to-mp3.test.ts",
    "start": "node dist/server/index.js",
    "install:client": "cd client && npm install"
  },
  "keywords": [
    "youtube",
    "mp3",
    "converter",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@distube/ytdl-core": "^4.16.11",
    "@types/express-session": "^1.18.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "googleapis": "^128.0.0",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
